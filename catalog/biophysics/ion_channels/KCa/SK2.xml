<?xml version="1.0" encoding="UTF-8"?>
<NineML xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns="http://nineml.incf.org/9ML/0.3"
  xsi:schemaLocation="http://nineml.incf.org/9ML/NineML_v0.3.xsd">
  <ComponentClass name="SK2def">
    <Parameter name="diff" dimension="concentration"/>
    <Parameter name="invc1"/>
    <Parameter name="invc2"/>
    <Parameter name="invc3"/>
    <Parameter name="invo1"/>
    <Parameter name="invo2"/>
    <Parameter name="diro1"/>
    <Parameter name="diro2"/>
    <Parameter name="dirc2"/>
    <Parameter name="dirc3"/>
    <Parameter name="dirc4"/>
    <Parameter name="gbar" dimension="conductance_density"/>
    <AnalogPort mode="recv" name="cai" dimension="current_density"/>
    <AnalogPort mode="recv" name="ki" dimension="current_density"/>
    <AnalogPort mode="recv" name="ko" dimension="current_density"/>
    <AnalogPort mode="recv" name="celsius" dimension="temperature"/>
    <AnalogPort mode="recv" name="v" dimension="voltage"/>
    <AnalogPort mode="send" name="i" dimension="current_density"/>
    <Dynamics>
      <Alias name="i">
       <MathInline>gbar * sum(o1,o2)(v - e_revseral)</MathInline>
      </Alias>
      <Regime>
        <Alias name="parity_check">
          <MathInline> (c1 + c2 + c3 + c4 + o2 + o1) </MathInline>
        </Alias>
        <TimeDerivative variable="c1">
          <MathInline> - from_c1_to_c2</MathInline>
        </TimeDerivative>
        <TimeDerivative variable="c2">
          <MathInline> + from_c1_to_c2</MathInline>
        </TimeDerivative>
        <TimeDerivative variable="c2">
          <MathInline> - from_c2_to_c1</MathInline>
        </TimeDerivative>
        <TimeDerivative variable="c1">
          <MathInline> + from_c2_to_c1</MathInline>
        </TimeDerivative>
        <TimeDerivative variable="c2">
          <MathInline> - from_c2_to_c3</MathInline>
        </TimeDerivative>
        <TimeDerivative variable="c3">
          <MathInline> + from_c2_to_c3</MathInline>
        </TimeDerivative>
        <TimeDerivative variable="c3">
          <MathInline> - from_c3_to_c2</MathInline>
        </TimeDerivative>
        <TimeDerivative variable="c2">
          <MathInline> + from_c3_to_c2</MathInline>
        </TimeDerivative>
        <TimeDerivative variable="c3">
          <MathInline> - from_c3_to_c4</MathInline>
        </TimeDerivative>
        <TimeDerivative variable="c4">
          <MathInline> + from_c3_to_c4</MathInline>
        </TimeDerivative>
        <TimeDerivative variable="c4">
          <MathInline> - from_c4_to_c3</MathInline>
        </TimeDerivative>
        <TimeDerivative variable="c3">
          <MathInline> + from_c4_to_c3</MathInline>
        </TimeDerivative>
        <TimeDerivative variable="c3">
          <MathInline> - from_c3_to_o1</MathInline>
        </TimeDerivative>
        <TimeDerivative variable="o1">
          <MathInline> + from_c3_to_o1</MathInline>
        </TimeDerivative>
        <TimeDerivative variable="o1">
          <MathInline> - from_o1_to_c3</MathInline>
        </TimeDerivative>
        <TimeDerivative variable="c3">
          <MathInline> + from_o1_to_c3</MathInline>
        </TimeDerivative>
        <TimeDerivative variable="c4">
          <MathInline> - from_c4_to_o2</MathInline>
        </TimeDerivative>
        <TimeDerivative variable="o2">
          <MathInline> + from_c4_to_o2</MathInline>
        </TimeDerivative>
        <TimeDerivative variable="o2">
          <MathInline> - from_o2_to_c4</MathInline>
        </TimeDerivative>
        <TimeDerivative variable="c4">
          <MathInline> + from_o2_to_c4</MathInline>
        </TimeDerivative>
        <Alias name="from_c1_to_c2">
          <MathInline>dirc2_t_ca * c1</MathInline>
        </Alias>
        <Alias name="from_c2_to_c1">
          <MathInline>invc1_t * c2</MathInline>
        </Alias>
        <Alias name="from_c2_to_c3">
          <MathInline>dirc3_t_ca * c2</MathInline>
        </Alias>
        <Alias name="from_c3_to_c2">
          <MathInline>invc2_t * c3</MathInline>
        </Alias>
        <Alias name="from_c3_to_c4">
          <MathInline>dirc4_t_ca * c3</MathInline>
        </Alias>
        <Alias name="from_c4_to_c3">
          <MathInline>invc3_t * c4</MathInline>
        </Alias>
        <Alias name="from_c3_to_o1">
          <MathInline>diro1_t * c3</MathInline>
        </Alias>
        <Alias name="from_o1_to_c3">
          <MathInline>invo1_t * o1</MathInline>
        </Alias>
        <Alias name="from_c4_to_o2">
          <MathInline>diro2_t * c4</MathInline>
        </Alias>
        <Alias name="from_o2_to_c4">
          <MathInline>invo2_t * o2</MathInline>
        </Alias>
        <Alias name="Q10">
          <MathInline>pow(3, ((celsius - 23) / 10))</MathInline>
        </Alias>
        <Alias name="invc1_t">
          <MathInline>invc1 * Q10</MathInline>
        </Alias>
        <Alias name="invc2_t">
          <MathInline>invc2 * Q10</MathInline>
        </Alias>
        <Alias name="invc3_t">
          <MathInline>invc3 * Q10</MathInline>
        </Alias>
        <Alias name="invo1_t">
          <MathInline>invo1 * Q10</MathInline>
        </Alias>
        <Alias name="invo2_t">
          <MathInline>invo2 * Q10</MathInline>
        </Alias>
        <Alias name="diro1_t">
          <MathInline>diro1 * Q10</MathInline>
        </Alias>
        <Alias name="diro2_t">
          <MathInline>diro2 * Q10</MathInline>
        </Alias>
        <Alias name="dirc2_t">
          <MathInline>dirc2 * Q10</MathInline>
        </Alias>
        <Alias name="dirc3_t">
          <MathInline>dirc3 * Q10</MathInline>
        </Alias>
        <Alias name="dirc4_t">
          <MathInline>dirc4 * Q10</MathInline>
        </Alias>
        <Alias name="dirc2_t_ca">
          <MathInline>dirc2_t * (cai / diff)</MathInline>
        </Alias>
        <Alias name="dirc3_t_ca">
          <MathInline>dirc3_t * (cai / diff)</MathInline>
        </Alias>
        <Alias name="dirc4_t_ca">
          <MathInline>dirc4_t * (cai / diff)</MathInline>
        </Alias>
        <Alias name="e_reversal">
          <MathInline>log(ko / ki) * 8.314 * (celsius + 273.15) /
            (96485)</MathInline>
        </Alias>
      </Regime>
    </Dynamics>
  </ComponentClass>
</NineML>
